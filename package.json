{
  "name": "alex",
  "version": "0.0.1",
  "main": "main.js",
  "description": "AnaLysis of EXecution",
  "repository": {
    "type": "git",
    "url": "https://github.com/curtsinger-lab/alex"
  },
  "scripts": {
    "start": "node .",
    "precommit": "lint-staged",
    "eslint:fix": "eslint --fix .",
    "clean:outfiles": "rm -f out-* err-* result-*",
    "example": "npm run clean:outfiles && node . collect --in examples/inputs/matrix-500x500.in -- examples/build/matrixmultiplier",
    "checkpreset": "./collector/available-events",
    "checkpreset:energy": "./collector/available-events energy 2>> err-available-event.log",
    "checkpreset:cache": "./collector/available-events cache 2>> err-available-event.log",
    "checkpreset:cpu": "./collector/available-events cpu 2>> err-available-event.log",
    "checkpreset:branch": "./collector/available-events branch 2>> err-available-event.log",
    "checkpreset:all": "./collector/available-events all 2>> err-available-event.log"
  },
  "bin": {
    "alex": "main.js"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "chi-squared": "^1.1.0",
    "d3": "^5.4.0",
    "d3-svg-legend": "^2.25.6",
    "electron": "^2.0.2",
    "husky": "^0.14.3",
    "jquery": "^3.3.1",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.10",
    "popper.js": "^1.14.3",
    "prettier": "^1.13.5",
    "progressbar.js": "^1.0.1",
    "tempfile": "^2.0.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/yargs": "^11.0.0",
    "eslint": "^4.19.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "no-console": "off",
      "no-var": "error",
      "eqeqeq": "error",
      "prefer-const": "warn",
      "prefer-arrow-callback": "warn",
      "arrow-body-style": "warn",
      "no-unused-vars": "warn",
      "no-empty": "warn"
    }
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{c,h,cpp,hpp}": [
      "clang-format -i -style=file",
      "git add"
    ]
  }
}
